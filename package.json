{
  "name": "vue3-boilerplate",
  "description": "Quick boilerplate Vue3,Pinia,VueRouter,Tailwind,Icons,Typescript",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 4173",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts --fix --ignore-path .gitignore && vue-tsc --noEmit",
    "lint:format": "prettier --check --ignore-path .gitignore '**/*.{js,jsx,vue,ts}'",
    "format": "prettier --write --ignore-path .gitignore '**/*.{js,jsx,vue,ts}'"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "pinia": "^2.0.35",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.21.5",
    "@heroicons/vue": "^2.0.17",
    "@rushstack/eslint-patch": "^1.2.0",
    "@types/lodash": "^4.14.194",
    "@vitejs/plugin-vue": "^4.2.1",
    "@vue/cli-plugin-typescript": "^5.0.8",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "autoprefixer": "^10.4.14",
    "cssnano": "^5.1.15",
    "eslint": "^8.39.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.11.0",
    "postcss": "^8.4.23",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.8",
    "sass": "^1.62.1",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.5",
    "vite": "^4.3.3",
    "vite-svg-loader": "^4.0.0",
    "vue-tsc": "^1.6.1"
  }
}
